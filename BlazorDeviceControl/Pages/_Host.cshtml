@page "/"
@namespace BlazorDeviceControl.Pages
@using BlazorCore.Models
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "_Layout";
}

<component type="typeof(App)" render-mode="ServerPrerendered" />

@{
    UserSettingsHelper.Instance.Identity.IpAddress = HttpContext.Connection.RemoteIpAddress?.ToString();
    UserSettingsHelper.Instance.Identity.Id = HttpContext.Connection.Id;
}
