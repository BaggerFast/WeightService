<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Version>$(VersionPrefix)</Version>
    <Authors>Владимирский стандарт</Authors>
    <Company>Владимирский стандарт</Company>
    <UserSecretsId>993a2cab-36f2-4b69-a954-68c43db890b1</UserSecretsId>
    <StartupObject>BlazorDeviceControl.Program</StartupObject>
    <Product>WeightService</Product>
    <Copyright>Copyright © 2020 - 2022</Copyright>
	<AssemblyName>BlazorDeviceControl</AssemblyName>
    <RootNamespace>BlazorDeviceControl</RootNamespace>
    <NeutralLanguage>ru-RU</NeutralLanguage>
	<FileVersion>0.6.430</FileVersion>
    <AssemblyVersion>0.6.430</AssemblyVersion>
    <Platforms>AnyCPU;x64</Platforms>
    <Configurations>Debug;Release;Dev</Configurations>
	<Nullable>enable</Nullable>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>none</DebugType>
    <DebugSymbols>false</DebugSymbols>
    <Optimize>True</Optimize>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <DebugType>none</DebugType>
    <DebugSymbols>false</DebugSymbols>
    <Optimize>True</Optimize>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <Optimize>False</Optimize>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Data\**" />
    <Compile Remove="Upload\**" />
    <Content Remove="Data\**" />
    <Content Remove="Upload\**" />
    <EmbeddedResource Remove="Data\**" />
    <EmbeddedResource Remove="Upload\**" />
    <None Remove="Data\**" />
    <None Remove="Upload\**" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="wwwroot\Images\" />
    <Folder Include="wwwroot\static\lib\" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="BlazorDownloadFile" Version="2.3.1.1" />
    <PackageReference Include="BlazorInputFile" Version="0.2.0" />
    <PackageReference Include="FluentNHibernate" Version="3.1.0" />
    <PackageReference Include="Microsoft.AspNetCore" Version="2.2.0" />
    <PackageReference Include="Microsoft.AspNetCore.Authentication.Negotiate" Version="6.0.3" />
    <PackageReference Include="Microsoft.AspNetCore.Authorization" Version="6.0.3" />
    <PackageReference Include="Microsoft.AspNetCore.Components" Version="6.0.3" />
    <PackageReference Include="Microsoft.AspNetCore.Components.Authorization" Version="6.0.3" />
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="6.0.3" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="6.0.3" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.Authentication" Version="6.0.3" />
    <PackageReference Include="Microsoft.Data.SqlClient" Version="4.1.0" />
    <PackageReference Include="MudBlazor" Version="6.0.8" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="NHibernate" Version="5.3.11" />
    <PackageReference Include="Radzen.Blazor" Version="3.16.7" />
    <PackageReference Include="RestSharp" Version="107.3.0" />
  </ItemGroup>
  <ItemGroup>
    <None Include="appsettings.json" />
    <None Include="appsettings.Debug.json">
      <DependentUpon>appsettings.json</DependentUpon>
    </None>
    <None Include="appsettings.Release.json">
      <DependentUpon>appsettings.json</DependentUpon>
    </None>
  </ItemGroup>
  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>
  <ItemGroup>
      <Watch Include="..\**\*.razor;..\**\*.scss;..\**\*.css;..\**\*.html;" />
  </ItemGroup>
  <ItemGroup>
    <Watch Remove="Shared\Item\ItemAccess.razor" />
    <Watch Remove="Shared\Item\ItemBarCode.razor" />
    <Watch Remove="Shared\Item\ItemBarCodeType.razor" />
    <Watch Remove="Shared\Item\ItemContragent.razor" />
    <Watch Remove="Shared\Item\ItemError.razor" />
    <Watch Remove="Shared\Item\ItemHost.razor" />
    <Watch Remove="Shared\Item\ItemLabel.razor" />
    <Watch Remove="Shared\Item\ItemLog.razor" />
    <Watch Remove="Shared\Item\ItemNomenclature.razor" />
    <Watch Remove="Shared\Item\ItemPlu.razor" />
    <Watch Remove="Shared\Item\ItemPrinter.razor" />
    <Watch Remove="Shared\Item\ItemPrinterResource.razor" />
    <Watch Remove="Shared\Item\ItemPrinterType.razor" />
    <Watch Remove="Shared\Item\ItemProductionFacility.razor" />
    <Watch Remove="Shared\Item\ItemScale.razor" />
    <Watch Remove="Shared\Item\ItemTaskModule.razor" />
    <Watch Remove="Shared\Item\ItemTemplate.razor" />
    <Watch Remove="Shared\Item\ItemTemplateResource.razor" />
    <Watch Remove="Shared\Item\ItemWorkshop.razor" />
    <Watch Remove="Shared\Section\SectionAccess.razor" />
    <Watch Remove="Shared\Section\SectionBarCodes.razor" />
    <Watch Remove="Shared\Section\SectionBarCodeTypes.razor" />
    <Watch Remove="Shared\Section\SectionContragents.razor" />
    <Watch Remove="Shared\Section\SectionErrors.razor" />
    <Watch Remove="Shared\Section\SectionHosts.razor" />
    <Watch Remove="Shared\Section\SectionInfo.razor" />
    <Watch Remove="Shared\Section\SectionLabels.razor" />
    <Watch Remove="Shared\Section\SectionLogs.razor" />
    <Watch Remove="Shared\Section\SectionNomenclatures.razor" />
    <Watch Remove="Shared\Section\SectionOrders.razor" />
    <Watch Remove="Shared\Section\SectionOrderStatuses.razor" />
    <Watch Remove="Shared\Section\SectionOrderTypes.razor" />
    <Watch Remove="Shared\Section\SectionPlus.razor" />
    <Watch Remove="Shared\Section\SectionPrinterResources.razor" />
    <Watch Remove="Shared\Section\SectionPrinters.razor" />
    <Watch Remove="Shared\Section\SectionPrinterTypes.razor" />
    <Watch Remove="Shared\Section\SectionProductionFacilities.razor" />
    <Watch Remove="Shared\Section\SectionProductSeries.razor" />
    <Watch Remove="Shared\Section\SectionScales.razor" />
    <Watch Remove="Shared\Section\SectionTaskModules.razor" />
    <Watch Remove="Shared\Section\SectionTemplateResources.razor" />
    <Watch Remove="Shared\Section\SectionTemplates.razor" />
    <Watch Remove="Shared\Section\SectionWeithingFacts.razor" />
    <Watch Remove="Shared\Section\SectionWorkshops.razor" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\BlazorCore\BlazorCore.csproj" />
    <ProjectReference Include="..\DataCore\DataCore.csproj" />
  </ItemGroup>
  <Target Name="AfterBuild" BeforeTargets="PrepareForBuild">
    <TransformXml Source="appsettings.json" Transform="appsettings.$(Configuration).json" Destination="appsettings.json" StackTrace="true" />
  </Target>
  <PropertyGroup>
    <LangVersion>9.0</LangVersion>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <Optimize>False</Optimize>
  </PropertyGroup>
</Project>
