<Application
	x:Class="TscPrintDemoWpf.App"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:barcodePrintUtils="clr-namespace:MDSoft.BarcodePrintUtils;assembly=MDSoft.BarcodePrintUtils"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
	xmlns:tsc="clr-namespace:MDSoft.BarcodePrintUtils.Tsc;assembly=MDSoft.BarcodePrintUtils"
	xmlns:enums="clr-namespace:WsPrintCore.Common;assembly=WsPrintCore"
	StartupUri="Views/MainWindow.xaml">

	<Application.Resources>
		<tsc:TscDriverHelper x:Key="PrintControlEntityViewModel" />
		<ObjectDataProvider
			x:Key="enumPrintLabelSize"
			MethodName="GetValues"
			ObjectType="{x:Type sys:Enum}">
			<ObjectDataProvider.MethodParameters>
				<x:Type TypeName="enums:PrintLabelSize" />
			</ObjectDataProvider.MethodParameters>
		</ObjectDataProvider>
		<ObjectDataProvider
			x:Key="enumPrintDpi"
			MethodName="GetValues"
			ObjectType="{x:Type sys:Enum}">
			<ObjectDataProvider.MethodParameters>
				<x:Type TypeName="enums:PrintLabelDpi" />
			</ObjectDataProvider.MethodParameters>
		</ObjectDataProvider>
		<ObjectDataProvider
			x:Key="enumPrintChannel"
			MethodName="GetValues"
			ObjectType="{x:Type sys:Enum}">
			<ObjectDataProvider.MethodParameters>
				<x:Type TypeName="enums:PrintChannel" />
			</ObjectDataProvider.MethodParameters>
		</ObjectDataProvider>
	</Application.Resources>
</Application>
