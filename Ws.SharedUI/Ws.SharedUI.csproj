<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <EmbeddedResource Update="Resources\WsDataResources.resx">
        <Generator>PublicResXFileCodeGenerator</Generator>
        <LastGenOutput>WsDataResources.Designer.cs</LastGenOutput>
      </EmbeddedResource>
      <EmbeddedResource Update="Resources\WsDataResources.ru-RU.resx">
        <Generator>PublicResXFileCodeGenerator</Generator>
        <LastGenOutput>WsDataResources.ru-RU.Designer.cs</LastGenOutput>
      </EmbeddedResource>
      <EmbeddedResource Update="Resources\WsDataResources.en-US.resx">
        <Generator>PublicResXFileCodeGenerator</Generator>
        <LastGenOutput>WsDataResources.en-US.Designer.cs</LastGenOutput>
      </EmbeddedResource>
    </ItemGroup>

    <ItemGroup>
      <Compile Update="Resources\WsDataResources.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>WsDataResources.resx</DependentUpon>
      </Compile>
      <Compile Update="Resources\WsDataResources.ru-RU.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>WsDataResources.ru-RU.resx</DependentUpon>
      </Compile>
      <Compile Update="Resources\WsDataResources.en-US.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>WsDataResources.en-US.resx</DependentUpon>
      </Compile>
    </ItemGroup>

</Project>
