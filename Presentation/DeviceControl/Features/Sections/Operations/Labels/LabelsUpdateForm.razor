@inherits SectionFormBase<Ws.Domain.Models.Entities.Print.LabelEntity>


<SectionFormWrapper
  ShareUrl="@($"{RouteUtils.SectionLabels}/{SectionEntity.IdentityValueUid}")"
  CreateDate="SectionEntity.CreateDt"
>
  <SectionFormInputText
    Value="@SectionEntity.IdentityValueUid.ToString()"
    Label="@Localizer["DataGridColumnUID"]"
    IsDisabled
    IsCopyable
  />
  <SectionFormInputText
    Value="@GetPluTypeTitle(SectionEntity.Plu.IsCheckWeight)"
    Label="@Localizer["DataGridColumnIsWeight"]"
    IsDisabled
  />
  <SectionFormInputText
    Value="@SectionEntity.Line.Name"
    Label="@Localizer["DataGridColumnLine"]"
    Link="@GetLineLink()"
    IsDisabled
  />
  <SectionFormInputText
    Value="@SectionEntity.Plu.DisplayName"
    Label="@Localizer["DataGridColumnPlu"]"
    Link="@GetPluLink()"
    IsDisabled
  />
  <SectionFormInputText
    Value="@Template.Title"
    Label="@Localizer["DataGridColumnTemplate"]"
    Link="@GetTemplateLink()"
    IsDisabled
  />
  <SectionFormInputText
    Value="@SectionEntity.ProductDt.ToString("dd.MM.yyyy HH:mm")"
    Label="@Localizer["DataGridColumnProductDt"]"
    IsDisabled
  />
  <SectionFormInputText
    Value="@SectionEntity.ExpirationDt.ToString("dd.MM.yyyy HH:mm")"
    Label="@Localizer["DataGridColumnExpirationDt"]"
    IsDisabled
  />
  <SectionFormInputText
    Value="@SectionEntity.Zpl"
    Label="Zpl"
    Size="SectionFormInputSizeEnum.ExtraLarge"
    IsDisabled
    IsCopyable
  />
</SectionFormWrapper>