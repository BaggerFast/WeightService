@using Blazor.Heroicons


<div class="h-12 w-12 rounded-md border border-black/[.1] overflow-hidden flex items-center justify-center @CustomClass">
  @if (!string.IsNullOrEmpty(IconName))
  {
    <Heroicon Name="@IconName" Type="@IconType" class="h-8 w-8" />
  }
  else
  {
    @ChildContent
  }
</div>