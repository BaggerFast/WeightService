@using DeviceControl2.Source.Widgets.Section
@using Microsoft.Extensions.Localization
@using Ws.Shared.Enums
@inherits SectionDialogBase<Ws.Domain.Models.Entities.Ref.LineEntity>


<SectionDialog DialogItem="DialogItem" TabsList="@TabsList">
  <Tab Id="main">
    <LinesUpdateForm />
  </Tab>
</SectionDialog>

@code {
  [Inject] private IStringLocalizer<ApplicationResources> Localizer { get; set; } = default!;

  protected override List<EnumTypeModel<string>> InitializeTabList() =>
  [
    new(Localizer["SectionLines"], "main"),
  ];
}