<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <DisableImplicitNamespaceImports>true</DisableImplicitNamespaceImports>
        <AssemblyVersion>1.1.5.0</AssemblyVersion>
        <FileVersion>1.1.5.0</FileVersion>
        <Nullable>enable</Nullable>
        <LangVersion>latest</LangVersion>
        <Platforms>x64</Platforms>
        <Configurations>DevelopVS;ReleaseVS</Configurations>
    </PropertyGroup>
    <ItemGroup>
        <Compile Remove="Serialization\Utils\**" />
        <EmbeddedResource Remove="Serialization\Utils\**" />
        <None Remove="Serialization\Utils\**" />
    </ItemGroup>
    <ItemGroup>
        <None Remove="Resources\exit_1.png" />
        <None Remove="Resources\exit_2.png" />
        <None Remove="Resources\ScalesUI.ico" />
        <None Remove="Resources\scales_1.gif" />
        <None Remove="Resources\scales_2.gif" />
        <None Remove="Resources\settings_1.png" />
        <None Remove="Resources\settings_2.png" />
    </ItemGroup>
    <ItemGroup>
        <EmbeddedResource Include="Resources\exit_1.png" />
        <EmbeddedResource Include="Resources\exit_2.png" />
        <EmbeddedResource Include="Resources\ScalesUI.ico" />
        <EmbeddedResource Include="Resources\scales_1.gif" />
        <EmbeddedResource Include="Resources\scales_2.gif" />
        <EmbeddedResource Include="Resources\settings_1.png" />
        <EmbeddedResource Include="Resources\settings_2.png" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="FrameworkExtract.System.Deployment" Version="4.0.30319" />
        <PackageReference Include="IndexRange" Version="1.0.3" />
        <PackageReference Include="MDSoft.Wmi" Version="0.2.50" />
        <PackageReference Include="Refractored.MvvmHelpers" Version="1.6.2" />
    </ItemGroup>
    <ItemGroup>
        <None Update="appsettings.DevelopVS.json">
            <DependentUpon>appsettings.json</DependentUpon>
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
            <CopyToPublishDirectory>Never</CopyToPublishDirectory>
        </None>
        <None Update="appsettings.json">
            <CopyToOutputDirectory>Never</CopyToOutputDirectory>
            <CopyToPublishDirectory>Never</CopyToPublishDirectory>
        </None>
        <None Update="appsettings.ReleaseVS.json">
            <DependentUpon>appsettings.json</DependentUpon>
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
            <CopyToPublishDirectory>Never</CopyToPublishDirectory>
        </None>
        <None Update="machine.config">
            <CopyToOutputDirectory>Never</CopyToOutputDirectory>
        </None>
    </ItemGroup>
    <ItemGroup>
        <Folder Include="ZplTemplates\" />
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="..\WsLocalizationCore\WsLocalizationCore.csproj" />
    </ItemGroup>
    <PropertyGroup>
        <Version>1.1.5.0</Version>
        <DebugType>portable</DebugType>
    </PropertyGroup>
    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='DevelopVS|x64'">
        <DebugType>none</DebugType>
    </PropertyGroup>
    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='ReleaseVS|x64'">
        <DebugType>none</DebugType>
    </PropertyGroup>
    <Target Name="PostBuild" AfterTargets="PostBuildEvent">
        <Exec Command="Post-build.cmd" />
    </Target>
</Project>
