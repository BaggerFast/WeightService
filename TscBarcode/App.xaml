<Application x:Class="TscBarcode.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:tsc="clr-namespace:WeightCore.Print.Tsc;assembly=WeightCore"
             xmlns:print="clr-namespace:WeightCore.Print;assembly=WeightCore"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <tsc:TscDriverHelper x:Key="PrintControlEntityViewModel" />
        <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="enumPrintLabelSize">
            <ObjectDataProvider.MethodParameters>
                <x:Type TypeName="print:PrintLabelSize"/>
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
        <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="enumPrintDpi">
            <ObjectDataProvider.MethodParameters>
                <x:Type TypeName="print:PrintDpi"/>
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
        <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="enumPrintChannel">
            <ObjectDataProvider.MethodParameters>
                <x:Type TypeName="print:PrintChannel"/>
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
    </Application.Resources>
</Application>
