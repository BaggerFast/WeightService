<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Version>0.8.260.0</Version>
    <LangVersion>latest</LangVersion>
    <Platforms>AnyCPU;x64</Platforms>
  </PropertyGroup>

  <ItemGroup>
    <_WebToolingArtifacts Remove="Properties\PublishProfiles\DEMO-DEV-PREVIEW.pubxml" />
    <_WebToolingArtifacts Remove="Properties\PublishProfiles\DEMO-DEV.pubxml" />
    <_WebToolingArtifacts Remove="Properties\PublishProfiles\DEMO-PROD-PREVIEW.pubxml" />
    <_WebToolingArtifacts Remove="Properties\PublishProfiles\DEMO-PROD.pubxml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="7.0.0" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.4.0" />
  </ItemGroup>

</Project>
