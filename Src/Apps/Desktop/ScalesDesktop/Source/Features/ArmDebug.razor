@inject ArmEndpoints ArmEndpoints

<UseParameterlessEndpoint Endpoint="@ArmEndpoints.ArmEndpoint" Context="query">
  @if (query.HasData)
  {
    <span class="text-right absolute text-gray-500 bottom-0 right-1 text-sm xl:text-base">
      @query.Data.Printer.Name
    </span>
  }
</UseParameterlessEndpoint>
