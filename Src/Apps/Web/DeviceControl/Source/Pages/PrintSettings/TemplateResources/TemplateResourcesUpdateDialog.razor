@using DeviceControl.Source.Widgets.Section
@inherits SectionDialogBase<Ws.Domain.Models.Entities.Print.ZplResource>


<SectionDialog DialogItem="DialogItem" TabsList="@TabsList">
  <Tab Id="@typeof(TemplateResourcesUpdateForm).ToString()">
    <TemplateResourcesUpdateForm />
  </Tab>
</SectionDialog>

@code {
  [Inject] private IStringLocalizer<ApplicationResources> Localizer { get; set; } = default!;

  protected override List<KeyValuePair<string, string>> InitializeTabList() =>
  [
    new(typeof(TemplateResourcesUpdateForm).ToString(), Localizer["SectionTemplateResources"])
  ];
}
