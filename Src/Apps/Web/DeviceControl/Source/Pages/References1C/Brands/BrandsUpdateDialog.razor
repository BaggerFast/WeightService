@inherits SectionDialogBase<Ws.DeviceControl.Models.Dto.References1C.Brands.BrandDto>

<SectionDialog DialogItem="DialogItem" TabsList="@TabsList">
  <Tab Id="@typeof(BrandsUpdateForm).ToString()">
    <BrandsUpdateForm />
  </Tab>
</SectionDialog>

@code {
  [Inject] private IStringLocalizer<ApplicationResources> Localizer { get; set; } = default!;

  protected override List<KeyValuePair<string, string>> InitializeTabList() =>
    [
      new(typeof(BrandsUpdateForm).ToString(), Localizer["SectionBrands"])
    ];
}
