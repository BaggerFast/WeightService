@inherits SectionDialogBase<Ws.DeviceControl.Models.Dto.Shared.PackageDto>

<SectionDialog DialogItem="DialogItem" TabsList="@TabsList">
  <Tab Id="@typeof(BoxesUpdateForm).ToString()">
    <BoxesUpdateForm />
  </Tab>
</SectionDialog>

@code {
  [Inject] private IStringLocalizer<ApplicationResources> Localizer { get; set; } = default!;

  protected override List<KeyValuePair<string, string>> InitializeTabList() =>
  [
    new(typeof(BoxesUpdateForm).ToString(), Localizer["SectionBoxes"])
  ];
}
