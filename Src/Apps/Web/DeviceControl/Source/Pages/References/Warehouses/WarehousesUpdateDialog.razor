@using Ws.DeviceControl.Models.Dto.References.Warehouses
@using Ws.DeviceControl.Models.Dto.References.Warehouses.Queries

@implements IDialogContentComponent<WarehouseDto>

<SectionDialog>
  <Tab Id="@typeof(WarehousesUpdateForm).ToString()" Title="@Localizer["SectionWarehouses"]">
    <WarehousesUpdateForm Model="Content" FormModel="@WarehouseMapper.DtoToUpdateDto(Content)" />
  </Tab>
</SectionDialog>

@code {
  [Inject] private IStringLocalizer<ApplicationResources> Localizer { get; set; } = default!;

  [Parameter] public WarehouseDto Content { get; set; } = default!;
}
