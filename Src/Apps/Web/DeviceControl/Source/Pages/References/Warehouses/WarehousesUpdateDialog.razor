@inherits SectionDialogBase<Ws.DeviceControl.Models.Dto.References.Warehouses.Queries.WarehouseDto>

<SectionDialog DialogItem="DialogItem" TabsList="@TabsList">
  <Tab Id="@typeof(WarehousesUpdateForm).ToString()">
    <WarehousesUpdateForm />
  </Tab>
</SectionDialog>

@code {
  [Inject] private IStringLocalizer<ApplicationResources> Localizer { get; set; } = default!;

  protected override List<KeyValuePair<string, string>> InitializeTabList() =>
  [
    new(typeof(WarehousesUpdateForm).ToString(), Localizer["SectionWarehouses"])
  ];
}
