<pages:WpfPageBase 
	x:Class="WsWeight.Wpf.Pages.WpfPageMessageBox"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:pages="clr-namespace:WsWeight.Wpf.Pages"
    mc:Ignorable="d" 
    d:DesignWidth="640" d:DesignHeight="300" FontSize="20"
    Loaded="UserControl_Loaded" 
    DataContext="{DynamicResource UserSession}" >

	<pages:WpfPageBase.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="WpfResourceDictionary.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</pages:WpfPageBase.Resources>

	<Border Name="borderMain" KeyUp="Button_KeyUp" 
	        BorderBrush="Blue" BorderThickness="1" Margin="2" >
    </Border>

</pages:WpfPageBase>
